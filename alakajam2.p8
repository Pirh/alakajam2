-- title state

function titleinit()
  music(13, 200)
  level = 0
  fadecol = 0
  track = 0
  stoppoint = 10
  titlecols = {7, 7, 7, 7, 7}
  titlecolindex = 0
  titletimer = 0.0
end

function titleupdate()
  if btnp(4) or btnp(5) then
    changestate(states.game)
    music(0, 1000)
  end
  titletimer -= dt
  if titletimer < 0 then
    titletimer = 0.25
    local randomcolor = flr(rnd() * 15) + 1
    titlecols[(titlecolindex % 5) + 1] = randomcolor
    titlecolindex += 1
  end
end

function titledraw()
  cls(0)
  for i=1,15 do
    pal(i, 7)
  end
  for tcol in all(titlecols) do
    pal(tcol, 10)
  end
  sspr(0, 32, 40, 8, 24, 40, 80, 16)
  pal()
  print("press 🅾️ to start",
    31, 80, 10)
end
